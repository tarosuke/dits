{
	"name": "dits",
	"description": "dits for VSCode",
	"version": "0.0.0",
	"publisher": "tarosuke",
	"engines": {
		"vscode": "^1.32.0"
	},
	"activationEvents": [
		"onView:issue",
		"onView:log",
		"onView:children",
		"onCommand:newChild",
		"onCommand:refresh",
		"onCommand:openChild"
	],
	"main": "vscode/dits.js",
	"contributes": {
		"commands": [{
			"command": "dits.refresh",
			"title": "Refresh"
		},{
			"command": "dits.newChild",
			"title": "new"
		},{
			"command": "dits.openChild",
			"title": "open child"
		},{
			"command": "dits.finish",
			"title": "finish this issue"
		},{
			"command": "dits.delete",
			"title": "delete this issue"
		},{
			"command": "dits.goParent",
			"title": "go parent issue"
		}],
		"menus": {
			"view/title": [
				{
					"command": "dits.finish",
					"when": "view == issue",
					"group": "issue"
				},
				{
					"command": "dits.delete",
					"when": "view == issue",
					"group": "issue"
				},
				{
					"command": "dits.goParent",
					"when": "view == issue",
					"group": "issue"
				},
				{
					"command": "dits.newChild",
					"when": "view == children",
					"group": "children"
				}
			],
			"view/item/context": [
				{
					"command": "dits.openChild",
					"when": "view == children"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "dits",
					"title": "dits",
					"icon": "./resources/icon.svg"
				}
			]
		},
		"views": {
			"dits": [
				{
					"id": "issue",
					"name": "ISSUE"
				},
				{
					"id": "log",
					"name": "LOG"
				},
				{
					"id": "children",
					"name": "CHILDREN"
				},
				{
					"id": "closedChildren",
					"name": "CLOSED CHILDREN"
				}
			]
		}
	},
	"scripts": {},
	"devDependencies": {
		"@types/vscode": "^1.32.0"
	}
}
