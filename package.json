{
	"name": "dits",
	"description": "dits for VSCode",
	"version": "0.0.0",
	"publisher": "tarosuke",
	"engines": {
		"vscode": "^1.32.0"
	},
	"activationEvents": [
		"onView:log",
		"onView:children"
	],
	"main": "vscode/dits.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "issue",
					"title": "Issue",
					"icon": "./resources/icon.svg"
				}
			]
		},
		"views": {
			"issue": [
				{
					"id": "log",
					"name": "Issue"
				},
				{
					"id": "children",
					"name": "Children"
				}
			]
		}
	},
	"scripts": {},
	"devDependencies": {
		"@types/vscode": "^1.32.0"
	},
	"dependencies": {
		"wasm-git": "^0.0.4"
	}
}
